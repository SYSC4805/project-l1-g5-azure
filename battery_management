//Arkash sundralingam 101115738

char state;
int x;
void loop() { 
 battery_mangement();
 }

void battery_mangement(){
state='z';
//x=battery();
if (x>=60){
state='H';
//keep all on
}else if((x>=20)&&(x<=60)){
pinmode(pin(),low);
pinmode(pin(),low);
state=S
//Line + small block are on
}else{
pinmode(pin(),low);
pinmode(pin(),low);
//Line sensor is only on
state='L';
}
}
